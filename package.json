{
	"name": "php-transform-array",
	"displayName": "PHP Transform Array",
	"description": "Transform PHP Array old syntax to the new one",
	"author": "Yannick Fran√ßois <y.francois54@gmail.com>",
	"version": "0.0.3",
	"publisher": "Yannick",
	"repository": {
		"type": "git",
		"url": "https://github.com/YannickDurden/php-transform-array"
	},
	"engines": {
		"vscode": "^1.49.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:php",
		"onCommand:php-transform-array.transformArray"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "php-transform-array.transformArray",
				"title": "PHP Transform array"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.49.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.6.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
